{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "description": "Elite basketball consultancy desktop app.",
  "author": "Marko Proleta / CourtEvo Vero",
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@nivo/bar": "^0.99.0",
    "@nivo/heatmap": "^0.99.0",
    "@nivo/line": "^0.99.0",
